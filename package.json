{
  "name": "expo-aes-universal-web",
  "version": "0.1.0",
  "description": "AES encryption/decryption module for Expo and web",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "expo",
    "aes",
    "encryption",
    "web"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "expo-aes-universal": "file:expo-aes-universal-0.1.1.tgz",
    "expo-crypto-universal": "^0.2.6"
  },
  "dependencies": {
    "@higayasuo/u8a-utils": "^1.0.1"
  }
}
