{
  "name": "aes-universal-web",
  "version": "0.1.6",
  "description": "Web implementation of aes-universal",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run typecheck && vite build",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "expo",
    "aes",
    "encryption",
    "web"
  ],
  "author": "Yasuo Higa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/higayasuo/aes-universal-web.git"
  },
  "bugs": {
    "url": "https://github.com/higayasuo/aes-universal-web/issues"
  },
  "homepage": "https://github.com/higayasuo/aes-universal-web#readme",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^1.3.1",
    "aes-universal-node": "^0.1.6",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@higayasuo/u8a-utils": "^1.0.1",
    "aes-universal": "^0.1.6"
  }
}
